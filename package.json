{
  "private": true,
  "scripts": {
    "dev": "npm run artisan:all && npm run development:admin && npm run development:client",
    "development:admin": "cross-env BUNDLE_ANALYZER=false && npm run env:dev:admin -- node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development:client": "cross-env BUNDLE_ANALYZER=false && npm run env:dev:client -- node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch:client": "cross-env BUNDLE_ANALYZER=true && npm run env:dev:client -- node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch:admin": "cross-env BUNDLE_ANALYZER=true && npm run env:dev:admin -- node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll:client": "npm run watch:client -- --watch-poll",
    "watch-poll:admin": "npm run watch:admin -- --watch-poll",
    "hot:client": "npm run env:dev:client -- node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot:admin": "npm run env:dev:admin -- node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run artisan:all && npm run production:admin && npm run production:client",
    "production:admin": "npm run env:prod:admin -- node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production:client": "npm run env:prod:client -- node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint:script": "eslint --ext .js,.vue resources/assets/js",
    "lint:script:fix": "eslint --ext .js,.vue resources/assets/js --fix",
    "lint:style": "stylelint resources/assets/sass/**/*.{vue,scss}",
    "test": "ava --verbose",
    "routes": "php artisan routes:generate",
    "artisan:all": "npm run routes",
    "env:dev:client": "cross-env NODE_ENV=development BABEL_ENV=development APP_TYPE=client",
    "env:dev:admin": "cross-env NODE_ENV=development BABEL_ENV=development APP_TYPE=admin",
    "env:prod:client": "cross-env NODE_ENV=production BABEL_ENV=production APP_TYPE=client BUNDLE_ANALYZER=false",
    "env:prod:admin": "cross-env NODE_ENV=production BABEL_ENV=production APP_TYPE=admin BUNDLE_ANALYZER=false"
  },
  "dependencies": {
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "bootstrap": "4.1.3",
    "bootstrap-colorpicker": "3.0.3",
    "bootstrap-datepicker": "1.8.0",
    "bootstrap-maxlength": "1.6.0",
    "bootstrap-notify": "3.1.3",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "5.2.0",
    "datatables.net": "1.10.19",
    "datatables.net-bs4": "1.10.19",
    "datatables.net-select": "1.2.7",
    "dropzone": "5.5.1",
    "fontfaceobserver": "^2.0.13",
    "glob": "7.1.2",
    "glob-all": "3.1.0",
    "image-webpack-loader": "4.3.1",
    "jquery": "3.3.1",
    "jquery-bootstrap-wizard": "1.4.2",
    "jquery-countto": "1.2.0",
    "jquery-scroll-lock": "3.1.3",
    "jquery-slimscroll": "1.3.8",
    "jquery-validation": "1.17.0",
    "jquery.appear": "1.0.1",
    "jquery.maskedinput": "1.4.1",
    "js-cookie": "2.2.0",
    "laravel-mix": "2.1.11",
    "lodash": "4.17.10",
    "moment-locales-webpack-plugin": "1.0.7",
    "popper.js": "1.14.4",
    "postcss-preset-env": "5.3.0",
    "select2": "4.0.6-rc.1",
    "select2-bootstrap-css": "1.4.6",
    "sweetalert2": "7.26.10"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.6",
    "brotli-webpack-plugin": "^1.0.0",
    "browser-env": "^3.2.5",
    "browser-sync": "^2.24.6",
    "browser-sync-webpack-plugin": "^2.2.2",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-vue": "^4.7.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.2.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "ava": {
    "files": "resources/assets/js/**/*.spec.js",
    "require": [
      "babel-register",
      "./resources/assets/js/common/browserEnv.ava.js"
    ],
    "babel": "inherit"
  },
  "engine": {
    "node": ">=8.9.4",
    "npm": ">=5.8.0"
  },
  "browserslist": [
    "> 1%",
    "IE 10",
    "Firefox ESR",
    "not op_mini all"
  ]
}
